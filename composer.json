{
    "name": "yli/yli",
    "description": "Yli: Lightweight PHP framework.",
    "type": "project",
    "keywords": ["yli", "framework"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Yli\\Framework\\": "src/framework/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Yli\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "symfony/mailer": "^7.3",
        "egulias/email-validator": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.2",
        "phpstan/phpstan": "^2.1",
        "symfony/var-dumper": "^7.3",
        "phpunit/php-code-coverage": "^12.3",
        "monolog/monolog": "^3.9",
        "laravel/pint": "^1.23",
        "rector/rector": "^2.1"
    },
    "authors": [
        {
        "name": "Ashish Kumbhar",
        "email": "ashishkumbhar01@hotmail.com"
        }
    ],
    "scripts": {
        "test": "php ./vendor/bin/phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
